<Window x:Class="WpfTPV.View.TicketWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTPV.View"
        mc:Ignorable="d"
        Title="Ticket" Height="650" Width="450" ResizeMode="NoResize">
    <Grid x:Name="ticketGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <Image Source="/sources/churreria.jpg" Grid.Row="0"/>
        <Grid x:Name="ticketDescGrid" Grid.Row="1"/>
    </Grid>
</Window>
